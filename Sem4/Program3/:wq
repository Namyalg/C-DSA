#include<stdio.h>
#include<bits/stdc++.h>

using namespace std;


int main(){
    int edges;
    cout<<"Enter the number of edges ";
    cin>>edges;
    unordered_map<int, vector<int>> adj;
    for(int i = 0 ; i < edges; i++){
        int to, from;
        cout<<"Enter the FROM edge ";
        cin>>from;
        cout<<"Enter the TO EDGE ";
        cin>>to;
        adj[from].append(to);
        adj[to].append(from);
    }
    for(auto x : adj){
        cout<<"Edge is "<<x<<endl;
        for(auto y : x){
            cout<<y<<" ";
        }
        cout<<endl;
    }

    return 0;
}
